  "scripts": {
    "compile": "tsc -p ./ && webpack --mode production",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch"
